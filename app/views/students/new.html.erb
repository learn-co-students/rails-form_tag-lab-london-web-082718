<h3>Student Form</h3>

<form action="<%= students_path %>"  method="POST">

<%= form_tag students_path do %>
    <label>Add a new Student's First Name:</label><br>
    <%= text_field_tag :first_name %><br>

    <label>Add a new Student's Last Name:</label><br>
    <%= text_area_tag :last_name %><br>

    <%= submit_tag "Submit Student" %>
  <% end %>
</form>

<%= params.inspect %>
